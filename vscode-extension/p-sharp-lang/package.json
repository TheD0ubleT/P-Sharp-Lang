{
  "name": "p-sharp-lang",
  "displayName": "P-Sharp Lang",
  "description": "Provides language support for the P# (P-Sharp) language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "psc",
      "aliases": ["P#", "psc"],
      "extensions": [".psc"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "psc",
      "scopeName": "source.psharp",
      "path": "./syntaxes/psc.tmLanguage.json"
    }]
  }
}
